<!-- Latest posts's Template -->
        <template id="conecta_media_list_template" name="CONECTA media_list" inherit_id="snippet_latest_posts.media_list_template">
        	<xpath expr="//t[@t-foreach='posts']" position="replace">
                      <h3>Latest News</h3>
		      <t t-foreach="posts" t-as="p">
			<t t-set="properties" t-value="json.loads(p.cover_properties)"/>
                              <ul class="media-list">
                                   <li class="media"> <a t-att-href="'/blog/%s/post/%s' % (p.blog_id.id, p.id)" t-attf-class="media-photo loading_container thumb #{properties.get('background-color')}" t-attf-style="background-image: #{properties.get('background-image', 'none')}; opacity: #{properties.get('opacity', 1)};"></a> <a t-att-href="'/blog/%s/post/%s' % (p.blog_id.id, p.id)" class="media-date"><span t-field="p.create_date" t-field-options="{&quot;format&quot;: &quot;dd&quot;}"/><span t-field="p.create_date" t-field-options="{&quot;format&quot;: &quot;MMMM&quot;}"/></a>
                                        <h5 class="media-heading" t-field="p.name"></h5>
                                        <p t-field="p.subtitle" style="white-space: nowrap;overflow: hidden;text-overflow: ellipsis;max-width: 200px;"></p>
                                   </li>
                              </ul>
			</t>
        	</xpath>
        </template>

<!-- Latest posts's Template bib pictures -->
        <template id="conecta_media_list_template_big_picture" name="Conecta media_list" inherit_id="snippet_latest_posts.s_latest_posts_big_picture_template">
        	<xpath expr="//t[@t-foreach='posts']" position="replace">

						<div class="container lp-section-content-container lp-blog-grid-container">
        								<!--<div class="row">-->
       	
      <t t-foreach="posts" t-as="p">
        <t t-set="properties" t-value="json.loads(p.cover_properties)"/>

								<div class="col-md-4 col-sm-4 lp-blog-grid-box">
									<div class="lp-blog-grid-box-container lp-border lp-border-radius-8">
										<div class="lp-blog-grid-box-thumb">
											<div t-attf-class="thumb #{properties.get('background-color')}" t-attf-style="background-image: #{properties.get('background-image', 'none')}; opacity: 1; height: 250px; background-size: 100% 100%;"/>
										</div>
										<div class="lp-blog-grid-box-description text-center">
												<div class="lp-blog-user-thumb margin-top-subtract-25">
													
                    <t t-if="p.author_avatar">
                        <span t-field="p.author_avatar" class="o_not_editable" t-field-options="{&quot;widget&quot;: &quot;image&quot;}"/>
                    </t>													
													
												</div>
												<div class="lp-blog-grid-category">
													<a t-att-href="'/blog/%s' % (p.blog_id.id)">
														<span t-field="p.blog_id.name"/>
													</a>
												</div>
												<div class="lp-blog-grid-title">
													<h4 class="lp-h4">
														<a t-att-href="'/blog/%s/post/%s' % (p.blog_id.id, p.id)"><span t-field="p.name"/></a>
													</h4>
												</div>
												<ul class="lp-blog-grid-author">
													<li>
														<a href="#">
															<i class="fa fa-user"></i>
															<span t-field="p.author_id" style="display: inline-block;" t-field-options="{                                 &quot;widget&quot;: &quot;contact&quot;,                                 &quot;fields&quot;: [&quot;name&quot;]                             }"/>
														</a>
													</li>
													<li>
														<i class="fa fa-calendar"></i>
														<span t-field="p.create_date" t-field-options="{&quot;format&quot;: &quot;dd&quot;}"/> <span t-field="p.create_date" t-field-options="{&quot;format&quot;: &quot;MMMM&quot;}"/>
													</li>
												</ul><!-- ../lp-blog-grid-author -->
										</div>
									</div>
								</div>
						
      </t>
								
							<!--</div>-->
						</div><!-- ../section-content-container-->
        	</xpath>
        </template>


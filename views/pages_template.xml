					<li><span>Contact</span></li>
				</ul>
			</div>
			<div class="page-header-overlay"></div>
		</div>
	<!--==================================Section Open=================================-->
	<section class="clearfix">
		<div class="contact-left">
<div tabindex="0" style="position: relative;" id="map" class="contactmap leaflet-container leaflet-fade-anim">
			<div style="transform: translate(0px, 0px);" class="leaflet-map-pane">
			<div class="leaflet-tile-pane">
			<div class="leaflet-layer">
			<div style="" class="leaflet-tile-container leaflet-zoom-animated"></div>
			
			</div>
			</div>

			</div>
<div class="control tilt">
  <button class="dec">&#8601;</button>
  <button class="inc">&#8599;</button>
</div>

<div class="control rotation">
  <button class="inc">&#8630;</button>
  <button class="dec">&#8631;</button>
</div>

<div class="control zoom">
  <button class="dec">-</button>
  <button class="inc">+</button>
</div>
</div>		


<script src="/website_conecta/static/src/lib/OSMBuildings/OSMmap.js"></script>
			
		</div>
		<div  class=" padding-top-60 padding-bottom-70 contact-right">
			<h3 class=" lp-border-bottom padding-bottom-20 margin-bottom-20">Address</h3>
			<div class="address-box mr-bottom-30">
						<p>
							<i class="fa fa-map-marker"></i>
							<span>Praça do Município - 6230-000 Fundão</span>
						</p>
						<p>
							<i class="fa fa-phone"></i>
							<span>+351 2750 0000</span>
						</p>
						<p>
							<i class="fa fa-envelope"></i>
							<span>support@conecta.pt</span>
						</p>
				
					<ul class="social-icons post-socials contact-social">
						<li><a href="#"><!-- Facebook icon by Icons8 -->
							<img class="icon icons8-Facebook" src="data:image/png;base64," alt="facebook"/></a></li>
						<li><a href="#"><!-- Google Plus icon by Icons8 -->
							<img class="icon icons8-Google-Plus" src="data:image/png;base64," alt="google-plus"/></a></li>
						<li><a href="#"><!-- LinkedIn icon by Icons8 -->
							<img class="icon icons8-LinkedIn" src="data:image/png;base64," alt="linkedin"/></a></li>
						<li><a href="#"><!-- Instagram icon by Icons8 -->
							<img class="icon icons8-Instagram" src="data:image/png;base64," alt="instagram"/></a></li>
						<li><a href="#"><!-- Tumblr icon by Icons8 -->
							<img class="icon icons8-Tumblr" src="data:image/png;base64," alt="tumblr"/></a></li>
					</ul><!-- ../social-icons-->
			</div>

				    <div class="text-center mt64" name="mail_button">
				        <a t-attf-href="mailto:{{ res_company.email }}" class="btn btn-primary">Send us an email</a>
				    </div>				

		</div>
	</section>
	<!--==================================Section Close=================================-->


		      </div>
        	</xpath>
        </template>

        <template id="conecta_contact_form" name="CONECTA Contact form" inherit_id="website_crm.contactus_form">
        	<xpath expr="//form[@method='post']" position="replace">
<form action="/website_form/" method="post" data-model_name="crm.lead" data-success_page="/page/website_crm.contactus_thanks" class="s_website_form form-horizontal mt32" enctype="multipart/form-data">
                              <div class="form-group">
                                   <div class="col-sm-6 col-md-6">
                                        <input placeholder="Name" type="text" class="form-control o_website_form_input" name="contact_name" t-att-value="request.params.get('contact_name', '')" required=""/>
                                   </div>
                                   <div class="col-sm-6 col-md-6">
                                        <input placeholder="Phone" type="text" class="form-control o_website_form_input" name="phone" t-att-value="request.params.get('phone', '')"/>
                                   </div>
                              </div>
                              <div class="form-group">
                                   <div class="col-sm-6 col-md-6">
                                        <input placeholder="E-mail" type="text" class="form-control o_website_form_input" name="email_from" required="" t-att-value="request.params.get('email_from', '')"/>
                                   </div>
                                   <div class="col-sm-6 col-md-6">
                                        <input placeholder="Empresa/Instituição" type="text" class="form-control o_website_form_input" name="partner_name" required="" t-att-value="request.params.get('partner_name', '')"/>
                                   </div>
                              </div>
                              <div class="form-group">
                                   <div class="col-sm-12 col-md-12">
                                        <input placeholder="Subject" type="text" class="form-control o_website_form_input" name="name" required="" t-att-value="request.params.get('name', '')"/>
                                   </div>
                              </div>
                              <div class="form-group">
                                   <div class="col-sm-12 col-md-12">
                                        <textarea placeholder="Message" class="form-control o_website_form_input" name="description" required=""><t t-esc="request.params.get('description', '')"/></textarea>
                                   </div>
                                   <div class="col-sm-12 col-md-12"><br/>
                                        <button class="btn btn-primary btn-lg o_website_form_send">Send</button><span id="o_website_form_result"/> </div>
                              </div>
                         </form>
        	</xpath>
        </template>



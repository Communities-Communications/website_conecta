        <template id="conecta_contact" name="CONECTA Contact" inherit_id="website.contactus">
        	<xpath expr="//div[@id='wrap']" position="replace">
		      <div id="wrap">
			<div class="oe_structure"/>

	<div class="about-page-heading about-page">
